pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building World'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployment World'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing World'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing World'
            }
        }
    }
}
